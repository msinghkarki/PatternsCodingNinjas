/*
   1
  23
 345
4567
*/
#include <iostream>
using namespace std;

int main() {
  int n, i = 1, j;
  cout<<"enter no. of rows";
  cin >> n;
  while (i <= n) {
    int k = 1;
    while (k <= n - i) {
      cout << " ";
      k++;
    }
    int val = i;
    int j = 1;
    while (j <= i) {
      cout << val;
      val++;
      j++;
    }
    cout << endl;
    i++;
  }
  return 0;
}
