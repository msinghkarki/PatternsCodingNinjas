/*1
  23
  345
  4567*/
  
#include <iostream>
using namespace std;

int main() {
  int n, i = 1, j;
  cout<<"enter the no. of rows";
  cin >> n;
  while (i <= n) {
    int val = i;
    int j = 1;
    while (j <= i) {
      cout << val;
      val++;
      j++;
    }
    cout << endl;
    i++;
  }
  return 0;
}
