#include<iostream>
#include<cmath>
using namespace std;

int main()
{
    int n,i,j;
    cout<<"enter no. of rows";
    cin>>n;
    for(i=0;i<n;i++)
    {
    for(j=0;j<n;j++)
    {
    cout<<n;
    }
    cout<<endl;
    }
    return 0;
}
